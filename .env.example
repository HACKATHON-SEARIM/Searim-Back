# Database
DATABASE_URL=mysql+pymysql://user:password@localhost:3306/marine_real_estate

# Security
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# AI 모델 프로바이더 선택 (openai 또는 gemini)
AI_MODEL_PROVIDER=openai

# Gemini API
